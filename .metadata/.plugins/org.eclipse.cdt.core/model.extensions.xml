<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
	<extension id="ITI_Error_Parser" name="ITI_Error_Parser" point="org.eclipse.cdt.core.ErrorParser">
		<errorparser class="org.eclipse.cdt.core.errorparsers.RegexErrorParser" id="org.eclipse.cdt.ui.ITI_Error_Parser" name="ITI_Error_Parser">
			<pattern description-expr="$3" eat-processed-line="true" file-expr="$1" line-expr="$2" regex="(.+):(\d+):\d+: error: (.+)" severity="Error"/>
			<pattern description-expr="$3" eat-processed-line="true" file-expr="$1" line-expr="$2" regex="(.+):(\d+):\d+: warning: (.+)" severity="Warning"/>
		</errorparser>
	</extension>
</plugin>
